<div class="page-header">
  <h3>Bucket List #<%= @bucket_list.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Bucketlist</dt>
      <dd><%= @bucket_list.bucketlist %></dd>

      <dt>Users</dt>
      <dd><%= @bucket_list.users_id %></dd>

      <dt>Dishes</dt>
      <dd><%= @bucket_list.dishes_id %></dd>

      <dt>Selectedvenue</dt>
      <dd><%= @bucket_list.selectedvenue_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/bucket_lists" class="btn btn-primary">
        Back
      </a>
      <a href="/bucket_lists/<%= @bucket_list.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_bucket_list/<%= @bucket_list.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
