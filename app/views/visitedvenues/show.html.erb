<div class="page-header">
  <h3>Visitedvenue #<%= @visitedvenue.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd>
        <% if @visitedvenue.user.present? %>
          <a href="/users/<%= @visitedvenue.user_id %>">
            <%= @visitedvenue.user.email %>
          </a>
        <% end %>
      </dd>

      <dt>Venue</dt>
      <dd>
        <% if @visitedvenue.venue.present? %>
          <a href="/venues/<%= @visitedvenue.venue_id %>">
            <%= @visitedvenue.venue.restaurant %>
          </a>
        <% end %>
      </dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/visitedvenues" class="btn btn-primary">
        Back
      </a>
      <a href="/visitedvenues/<%= @visitedvenue.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_visitedvenue/<%= @visitedvenue.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
