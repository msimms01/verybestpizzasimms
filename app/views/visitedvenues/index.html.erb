<div class="page-header">
  <h1>
    Visitedvenues
    <a href="/visitedvenues/new" class="btn btn-lg btn-success">New Visitedvenue</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Venue</th>
        <th>Actions</th>
      </tr>

      <% @visitedvenues.each do |visitedvenue| %>
      <tr>
        <td><%= visitedvenue.user.email %></td>
        <td><%= visitedvenue.venue_id %></td>
        <td>
          <a href="/visitedvenues/<%= visitedvenue.id %>" class="btn btn-primary">Show</a>
          <a href="/visitedvenues/<%= visitedvenue.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_visitedvenue/<%= visitedvenue.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
